(function() {
  var clickDot, createDots, main, next, prev, showDialog, slideLoop;

  createDots = function() {
    var i, nav, slides;
    nav = $('#slider-nav ul');
    slides = $('.slide').length;
    i = 0;
    while (i < slides) {
      nav.append('<li class="dot">&bull;</li>');
      i++;
    }
    $('.dot').first().addClass('active-dot');
  };

  next = function() {
    var activeDot, activeSlide, nextDot, nextSlide;
    activeSlide = $('.active-slide');
    nextSlide = activeSlide.next('.slide');
    if (nextSlide.length === 0) {
      nextSlide = $('.slide').first();
    }
    activeSlide.fadeOut('slow').removeClass('active-slide');
    nextSlide.fadeIn(1000).addClass('active-slide');
    activeDot = $('.active-dot');
    nextDot = activeDot.next('.dot');
    if (nextDot.length === 0) {
      nextDot = $('.dot').first();
    }
    activeDot.removeClass('active-dot');
    nextDot.addClass('active-dot');
  };

  prev = function() {
    var activeDot, activeSlide, prevDot, prevSlide;
    activeSlide = $('.active-slide');
    prevSlide = activeSlide.prev('.slide');
    if (prevSlide.length === 0) {
      prevSlide = $('.slide').last();
    }
    activeSlide.fadeOut('slow').removeClass('active-slide');
    prevSlide.fadeIn(1000).addClass('active-slide');
    activeDot = $('.active-dot');
    prevDot = activeDot.prev('.dot');
    if (prevDot.length === 0) {
      prevDot = $('.dot').last();
    }
    activeDot.removeClass('active-dot');
    prevDot.addClass('active-dot');
  };

  clickDot = function(dot) {
    var activeSlide, i, nextSlide;
    activeSlide = $('.active-slide');
    i = $(dot).index() + 1;
    nextSlide = $('.slide:nth-child(' + i + ')');
    activeSlide.fadeOut('slow').removeClass('active-slide');
    nextSlide.fadeIn(1000).addClass('active-slide');
    $('.active-dot').removeClass('active-dot');
    $(dot).addClass('active-dot');
  };

  slideLoop = void 0;

  showDialog = function(show) {
    if (show) {
      $('#dialog').fadeIn('slow');
      clearInterval(slideLoop);
    } else {
      $('#dialog').fadeOut('slow');
    }
    return show;
  };

  main = function() {
    var dialog, mili;
    mili = 5000;
    dialog = false;
    createDots();
    slideLoop = setInterval(next, mili);
    $('#slider').mouseenter(function() {
      $('.arrow').fadeIn('slow');
      $('#update-btn').fadeIn('slow');
      if (!dialog) {
        clearInterval(slideLoop);
      }
    });
    $('#slider').mouseleave(function() {
      $('.arrow').fadeOut('slow');
      $('#update-btn').fadeOut('slow');
      if (!dialog) {
        slideLoop = setInterval(next, mili);
      }
    });
    $('.right').click(next);
    $('.left').click(prev);
    return $('.dot').click(function() {
      clickDot(this);
    });
  };

  $(document).ready(main);

}).call(this);
